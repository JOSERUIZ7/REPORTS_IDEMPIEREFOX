Parameters ORIGEN,DESTINO

*ON ERROR POLICE(ORIGEN,DESTINO)

XTAB_DES = "'" + DESTINO +"'"

IF !USED(DESTINO)
	PRG_ALIASFREE()
	USE &DESTINO
ENDIF


Select &ORIGEN
GO TOP
Scan While .T.
	Scatter Memvar Memo
	Insert Into &DESTINO From Memvar
Endscan

Select &DESTINO
GO TOP

*!*	ON ERROR

*!*	PROCEDURE POLICE
*!*	PARAMETERS ORIGEN,DESTINO
*!*	WAIT WINDOW ORIGEN
*!*	WAIT WINDOW DESTINO
