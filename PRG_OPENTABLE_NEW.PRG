Parameters XRESULT,XCSQL

If Empty(xOBJCONN) Or xOBJCONN = -1
	Wait Window 'NO SE PUEDE PROCESAR'
	RETURN .F.
Endif

XRES = SQLEXEC(xOBJCONN,XCSQL,XRESULT)

IF XRES = -1
	RETURN .F.
ELSE
	RETURN .T.
ENDIF

*!*	xOBJRS.Open(XCSQL,xOBJCONN,3)
*!*	nError = oVFPCOM.RSToCursor(xOBJRS, XRESULT)
*Select &XRESULT
*!*	xOBJRS.CLOSE
