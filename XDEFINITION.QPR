SELECT XDEFINITION.C_invoice_, XDEFINITION.D_week, SUM(XDEFINITION.Invoiceope);
    FROM 'XDEFINITION.DBF' XDEFINITION;
    GROUP BY XDEFINITION.C_invoice_, XDEFINITION.D_week;
    ORDER BY XDEFINITION.C_invoice_, XDEFINITION.D_week;
    INTO CURSOR SYS(2015)
DO (_GENXTAB) WITH 'Query',.t.,.t.,.t.,,,,.t.,0,.t.
BROWSE NOMODIFY